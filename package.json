{
  "name": "@kibis-is/web-extension",
  "version": "2.6.0-beta.3",
  "description": "An AVM wallet that goes beyond just DeFi.",
  "main": "build/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibis-is/web-extension"
  },
  "author": {
    "name": "Kieran O'Neill",
    "email": "kieran@kibis.is",
    "url": "https://github.com/kieranroneill"
  },
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "algorand",
    "algosdk",
    "avm",
    "blockchain",
    "browser",
    "chrome",
    "edge",
    "firefox",
    "opera",
    "voi",
    "wallet"
  ],
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "pnpm@10.3.0",
  "scripts": {
    "analyze:chrome:apps": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name extension-apps --env environment=production --env target=chrome --analyze",
    "analyze:firefox:apps": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name extension-apps --env environment=production --env target=firefox --analyze",
    "build:chrome": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name extension-scripts --config-name extension-apps --env environment=production --env target=chrome",
    "build:dapp-example": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name dapp-example --env environment=production",
    "build:edge": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name extension-scripts --config-name extension-apps --env environment=production --env target=edge",
    "build:firefox": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name extension-scripts --config-name extension-apps --env environment=production --env target=firefox",
    "build:opera": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name extension-scripts --config-name extension-apps --env environment=production --env target=opera",
    "check:types": "tsc --noEmit",
    "install:chrome": "./scripts/install_chrome.sh",
    "install:firefox": "./scripts/install_firefox.sh",
    "lint": "eslint .",
    "package:chrome": "./scripts/package.sh chrome",
    "package:edge": "./scripts/package.sh edge",
    "package:firefox": "./scripts/package.sh firefox",
    "package:opera": "./scripts/package.sh opera",
    "prepare": "husky && ./scripts/post_install.sh",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,json,ts,tsx}\"",
    "start:chrome": "concurrently --names \"DAPP,EXTENSION\" -c \"blue.bold,magenta.bold\" \"pnpm start:dapp-example\" \"pnpm start:extension --env target=chrome\"",
    "start:dapp-example": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack serve --config webpack/webpack.config.ts --config-name dapp-example",
    "start:edge": "pnpm start:extension --env target=edge",
    "start:extension": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.webpack.json\" webpack --config webpack/webpack.config.ts --config-name extension-scripts --config-name extension-apps --env environment=development --watch",
    "start:firefox": "concurrently --names \"DAPP,EXTENSION\" -c \"blue.bold,magenta.bold\" \"pnpm start:dapp-example\" \"pnpm start:extension --env target=firefox\"",
    "start:opera": "pnpm start:extension --env target=opera",
    "test": "exit 0",
    "test:coverage": "jest --coverage",
    "validate:firefox": "addons-linter .firefox_build/"
  },
  "devDependencies": {
    "@anolilab/semantic-release-pnpm": "^1.1.10",
    "@blockshake/defly-connect": "^1.1.6",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.26.0",
    "@perawallet/connect": "^1.3.4",
    "@perawallet/connect-beta": "^1.3.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.7",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@txnlab/use-wallet": "3.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "20.8.0",
    "@types/qrcode": "^1.5.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/spark-md5": "^3.0.4",
    "@types/webextension-polyfill": "^0.10.0",
    "@types/zxcvbn": "^4.4.1",
    "@walletconnect/modal": "^2.6.2",
    "@walletconnect/sign-client": "^2.15.2",
    "@walletconnect/types": "^2.15.2",
    "addons-linter": "^6.17.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^9.1.7",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "lute-connect": "^1.3.0",
    "prettier": "^3.5.3",
    "raw-loader": "^4.0.2",
    "semantic-release": "^24.1.0",
    "style-loader": "^3.3.3",
    "thread-loader": "^4.0.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "4.9.5",
    "typescript-eslint": "^8.31.1",
    "web-ext": "^8.2.0",
    "webpack": "^5.94.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@agoralabs-sh/algorand-provider": "^1.1.0",
    "@agoralabs-sh/avm-web-provider": "^1.7.0",
    "@agoralabs-sh/uuid": "^1.1.0",
    "@chakra-ui/anatomy": "2.2.2",
    "@chakra-ui/color-mode": "2.2.0",
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "2.8.2",
    "@chakra-ui/system": "2.6.2",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@faker-js/faker": "^7.6.0",
    "@noble/curves": "^1.8.1",
    "@noble/hashes": "^1.7.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@stablelib/base64": "^1.0.1",
    "@stablelib/hex": "^1.0.1",
    "@stablelib/random": "^1.0.2",
    "@stablelib/utf8": "^2.0.1",
    "algosdk": "^2.7.0",
    "bignumber.js": "^9.1.1",
    "buffer": "^6.0.3",
    "cbor2": "^1.12.0",
    "chakra-ui-steps": "2.0.4",
    "csstype": "^3.1.2",
    "dompurify": "^3.0.1",
    "framer-motion": "^10.0.1",
    "i18next": "^22.4.10",
    "jsqr": "^1.4.0",
    "numbro": "^2.4.0",
    "qrcode": "^1.5.1",
    "react": "18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^5.2.1",
    "react-loader-spinner": "^5.3.4",
    "react-markdown": "^9.0.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "scrypt-async": "^2.0.1",
    "spark-md5": "3.0.2",
    "tweetnacl": "^1.0.3",
    "webextension-polyfill": "^0.10.0",
    "zxcvbn": "^4.4.2"
  },
  "pnpm": {
    "overrides": {
      "ws@>=7.0.0 <7.5.10": ">=7.5.10",
      "image-size@>=1.1.0 <1.2.1": ">=1.2.1"
    },
    "ignoredBuiltDependencies": [
      "spawn-sync"
    ],
    "onlyBuiltDependencies": [
      "canvas"
    ]
  }
}
